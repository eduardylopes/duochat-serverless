{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "title": "Create Room Model",
    "type": "object",
    "properties": {
        "name": { "type": "string" },
        "maxUsers": { "type": "number", "minimum": 1, "maximum": 50 },
        "isPrivate": { "type": "boolean" },
        "password": { "type": "string" },
        "adminId": { "type": "string" },
        "lobbyId": { "type": "string" }
    },
    "required": ["name", "adminId", "isPrivate", "maxUsers", "lobbyId"]
}
